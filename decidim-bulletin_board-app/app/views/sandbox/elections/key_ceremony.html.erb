<%= link_to "Back", sandbox_elections_path %>

<h1>Key ceremony for <%= election.title %></h1>

<h2>Trustees</h2>

<table>
  <thead>
    <tr>
      <th>Unique ID</th>
      <th>Name</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% election.trustees.each do |trustee| %>
      <tr>
        <td><%= trustee.unique_id %></td>
        <td><%= trustee.name %></td>
        <td>
          <%= link_to "Start", "#" %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= javascript_include_tag "sandbox/key_ceremony" %>
