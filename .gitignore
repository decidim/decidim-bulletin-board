/decidim-bulletin-board-app/tmp/pids/*
!/decidim-bulletin-board-app/tmp/pids/
!/decidim-bulletin-board-app/tmp/pids/.keep

# Ignore uploaded files in development.
/decidim-bulletin-board-app/storage/*
!/decidim-bulletin-board-app/storage/.keep

